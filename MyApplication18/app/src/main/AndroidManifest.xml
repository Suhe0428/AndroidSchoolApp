<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="School">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.asus.login.LoginActivity">

        </activity>
        <activity android:name="com.example.asus.register.RegisterActivity" />
        <activity android:name="com.example.asus.register.RegisterSuccessActivity" />
        <activity android:name="com.example.asus.message.MessageActivity" />
        <activity android:name="com.example.asus.login.SignatureActivity" />
        <activity android:name="com.example.market.main.MarketActivity" />
        <activity android:name="com.example.market.item.ItemActivity" />
        <activity android:name="com.example.market.upload.UploadActivity" />
        <activity android:name="com.example.market.search.SearchActivity" />
        <activity android:name="com.example.help.main.HelpActivity" />
        <activity android:name="com.example.asus.login.RenameActivity" />
        <activity android:name="com.example.asus.community.post.PostItemActivity" />
        <activity android:name="com.example.asus.my.replace.StepOneActivity" />
        <activity android:name="com.example.asus.my.replace.StepTwoActivity" />
        <activity android:name="com.example.help.mytask.TaskMyActivity" />
        <activity android:name="com.example.help.publish.TaskPublishActivity" />
        <activity android:name="com.example.asus.my.myhelp.MyHelpActivity" />
        <activity android:name="com.example.asus.my.mymarket.MyMarketActivity" />
        <activity android:name="com.example.asus.my.mygeneral.MyGeneralActivity"></activity>
    </application>

</manifest>